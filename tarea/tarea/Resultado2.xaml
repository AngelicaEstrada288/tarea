<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="tarea.Resultado2">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Resultado Suma"/>
            <Label Text="{Binding suma}"
                 />

            <Label Text="Resultado Resta"/>
            <Label Text="{Binding restar}"
                 />
            
            <Label Text="Resultado Multiplicacion"/>
            <Label Text="{Binding multiplicar}"
                 />

            <Label Text="Resultado Divicion"/>
            <Label Text="{Binding dividir}"
                 />


        </StackLayout>
   </ContentPage.Content>

</ContentPage>